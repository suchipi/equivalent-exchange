{
  "name": "equivalent-exchange",
  "version": "1.7.0",
  "description": "Transmute one JavaScript string into another by way of mutating its AST. Powered by [babel](https://babeljs.io/) and [recast](https://www.npmjs.com/package/recast).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "tsc && cp dist/*.d.ts api/ && prettier --write api/index.d.ts"
  },
  "keywords": [
    "ast",
    "transform",
    "codemod",
    "babel",
    "recast",
    "transmute",
    "transmutation"
  ],
  "author": "Lily Scott <me@suchipi.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/suchipi/equivalent-exchange.git"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vitest": "^0.5.4"
  },
  "dependencies": {
    "@babel/core": "^7.18.6",
    "@babel/generator": "^7.18.7",
    "@babel/parser": "^7.18.8",
    "@babel/template": "^7.18.6",
    "@babel/traverse": "^7.18.8",
    "@babel/types": "^7.18.8",
    "@types/babel__core": "^7.1.19",
    "recast": "^0.21.1"
  }
}
